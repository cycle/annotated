{
  "name": "cycle/annotated",
  "type": "library",
  "license": "MIT",
  "description": "Cycle ORM Annotated Entities generator",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/rauanmayemir/spiral-database.git"
    }
  ],
  "config": {
    "preferred-install": {
      "spiral/database": "source",
      "*": "dist"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2",
    "spiral/tokenizer": "^2.1",
    "spiral/database": "dev-master#4f6a613 as 2.92.x-dev",
    "cycle/schema-builder": "^1.0",
    "doctrine/annotations": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "spiral/debug": "^1.3",
    "spiral/code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\Annotated\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Annotated\\Tests\\": "tests/Annotated/"
    }
  }
}
