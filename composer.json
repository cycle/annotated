{
  "name": "cycle/annotated",
  "type": "library",
  "license": "MIT",
  "description": "Cycle ORM Annotated Entities generator",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "spiral/tokenizer": "^2.8",
    "cycle/schema-builder": "2.0.x-dev",
    "doctrine/annotations": "^1.13",
    "spiral/attributes": "^2.8",
    "doctrine/inflector": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "spiral/code-style": "^1.0",
    "vimeo/psalm": "^4.12"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\Annotated\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Annotated\\Tests\\": "tests/Annotated/"
    }
  }
}
